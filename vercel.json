
{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    { "source": "/app.js", "headers": [{ "key": "Cache-Control", "value": "public, max-age=3600" }] },
    { "source": "/styles.css", "headers": [{ "key": "Cache-Control", "value": "public, max-age=3600" }] }
  ]
}
